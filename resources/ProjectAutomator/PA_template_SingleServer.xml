<?xml version="1.0" encoding="UTF-8" ?>
<DeployerSpec exitOnError="true" sourceType="Repository">
	<DeployerServer>
		<host>${deployer.host}:${deployer.port}</host>
		<user>${deployer.username}</user>
		<pwd>${deployer.password}</pwd>
	</DeployerServer>
	<Environment>
		<IS>
			<isalias name="${deployer.target.is.alias1}">
				<host>${deployer.target.is.host1}</host>
				<port>${deployer.target.is.port1}</port>
				<user>${deployer.target.is.user1}</user>
				<pwd>${deployer.target.is.password1}</pwd>
				<useSSL>false</useSSL>
				<version>${deployer.target.is.version}</version>
				<installDeployerResource>true</installDeployerResource>
				<Test>false</Test>
			</isalias>
		</IS>
		<MWS>
			<mwsalias name="${deployer.target.mws.alias1}">
				<host>${deployer.target.mws.host1}</host>
				<port>${deployer.target.mws.port1}</port>
				<user>${deployer.target.mws.user1}</user>
				<pwd>${deployer.target.mws.password1}</pwd>
				<useSSL>false</useSSL>
				<version>${deployer.target.mws.version}</version>
				<excludeCoreTaskEngineDependencies>true</excludeCoreTaskEngineDependencies>
				<cacheTimeOut>0</cacheTimeOut>
				<includeSecurityDependencies>true</includeSecurityDependencies>
				<rootFolderAliases>folder.public</rootFolderAliases>
				<maximumFolderObjectCount>8000</maximumFolderObjectCount>
				<enableAddtionalLogging>true</enableAddtionalLogging>
				<maxFolderDepth>25</maxFolderDepth>
				<Test>true</Test>
			</mwsalias>
		</MWS>
		<!-- 
		<RulesServer>
			<rulesserveralias name="${deployer.target.rule.alias1}">
				<host>${deployer.target.rule.host1}</host>
				<port>${deployer.target.rule.port1}</port>
				<user>${deployer.target.rule.user1}</user>
				<pwd>${deployer.target.rule.password1}</pwd>
				<useSSL>false</useSSL>
				<version>${deployer.target.rule.version}</version>
				<Test>false</Test>
			</rulesserveralias>
		</RulesServer>
		 -->
		<ProcessModel>
			<pmalias name="${deployer.target.processmodel.alias1}">
				<host>${deployer.target.processmodel.host1}</host>
				<port>${deployer.target.processmodel.port1}</port>
				<user>${deployer.target.processmodel.user1}</user>
				<pwd>${deployer.target.processmodel.password1}</pwd>
				<useSSL>false</useSSL>
				<version>${deployer.target.processmodel.version}</version>
				<Test>false</Test>
			</pmalias>
		</ProcessModel>
		<Repository>
			<repalias name="${repoName}">
				<type>FlatFile</type>
				<urlOrDirectory>${deployer.fbr.location}</urlOrDirectory>
				<Test>true</Test>
			</repalias>
		</Repository>
	</Environment>

	<Projects>
		<Project description="Created: ${currentDate}"
			ignoreMissingDependencies="true" name="${bda.deployer.projectName}"
			overwrite="true" type="Repository">

			<DeploymentSet autoResolve="ignore" description=""
				name="myDeploymentSet" srcAlias="${repoName}">
				<!-- Add all packages here -->
				<!-- Example for an IntegrationServer package: <Composite srcAlias="${repoName}" 
					type="IS" displayName="Package/ZfinDebiWsAccess" name="ZfinDebiWsAccess" 
					/> -->
			</DeploymentSet>

			<DeploymentMap description="" name="myDeploymentMap" />
			<MapSetMapping mapName="myDeploymentMap" setName="myDeploymentSet">
				<alias type="IS">${deployer.target.is.alias1}</alias>
				<alias type="ProcessModel">${deployer.target.processmodel.alias1}</alias>
				<!--<alias type="RULES">${deployer.target.rule.alias1}</alias>-->
				<alias type="MWS">${deployer.target.mws.alias1}</alias>
			</MapSetMapping>
			<DeploymentCandidate description=""
				mapName="myDeploymentMap" name="myDeployment" />
		</Project>
	</Projects>
</DeployerSpec>
